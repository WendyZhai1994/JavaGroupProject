public class Student
{
private int studentID;
private String studentName;
private String gender;
private String major;
private ArrayList<Questions> testquestions;
public ArrayList<Questions> getTestPaper(int number, String testtype)
{ArrayList<Questions> testq=new ArrayList<Questions>();

switch(testtype)
{
case "easy":  select ? from Questions where type= ?；
case "":   ；
case "":   ；
case "":   ；
}
this.testquestions=testq;
}
public int answerQuestions(ArrayList<String> answers)
{int rightNumber;
for(Questions q,String s:this.testquestions,answers)
{if(q.answer.equalto(s))
{rightNumber++;}}
}
}
}
